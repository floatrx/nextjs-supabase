{
  "name": "next-supabase",
  "description": "Next.js Blog with Supabase",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "gen:types": "pwd && source .env && supabase gen types typescript --project-id=$SUPABASE_PROJECT_ID > types/supabase.ts",
    "eslint": "next lint --fix",
    "type:check": "tsc --noEmit",
    "type:build": "tsc -b",
    "lint": "lint-staged",
    "pretty": "prettier --write  '**/*.{ts,tsx}'",
    "prepare": "husky",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts?(x)": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@heroui/accordion": "2.2.27",
    "@heroui/avatar": "2.2.25",
    "@heroui/breadcrumbs": "2.2.24",
    "@heroui/button": "2.2.30",
    "@heroui/card": "2.2.27",
    "@heroui/chip": "2.2.24",
    "@heroui/dropdown": "2.3.30",
    "@heroui/image": "2.2.19",
    "@heroui/input": "2.4.31",
    "@heroui/link": "2.2.25",
    "@heroui/modal": "2.2.27",
    "@heroui/navbar": "2.2.28",
    "@heroui/pagination": "2.2.26",
    "@heroui/popover": "2.3.30",
    "@heroui/select": "2.4.31",
    "@heroui/shared-icons": "2.1.10",
    "@heroui/snippet": "2.2.31",
    "@heroui/spinner": "2.2.27",
    "@heroui/switch": "2.2.26",
    "@heroui/system": "2.4.26",
    "@heroui/tabs": "2.2.27",
    "@heroui/theme": "2.4.26",
    "@hookform/resolvers": "5.2.2",
    "@mdxeditor/editor": "3.52.3",
    "@react-aria/ssr": "3.9.10",
    "@react-aria/visually-hidden": "3.8.30",
    "@supabase/auth-js": "2.95.3",
    "@supabase/postgrest-js": "2.95.3",
    "@supabase/ssr": "0.8.0",
    "@supabase/supabase-js": "2.95.3",
    "autoprefixer": "10.4.24",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "intl-messageformat": "11.1.2",
    "lucide-react": "0.563.0",
    "motion": "12.34.0",
    "next": "16.1.6",
    "next-themes": "0.4.6",
    "nextjs-toploader": "3.9.17",
    "nuqs": "2.8.8",
    "postcss": "8.5.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "7.71.1",
    "react-markdown": "10.1.0",
    "react-syntax-highlighter": "16.1.0",
    "rehype-raw": "7.0.0",
    "remark-gfm": "4.0.1",
    "server-only": "0.0.1",
    "sonner": "2.0.7",
    "swr": "2.4.0",
    "tailwind-merge": "3.4.0",
    "tailwind-variants": "3.2.2",
    "tailwindcss-animate": "1.0.7",
    "zod": "3.25.76",
    "zod-validation-error": "3.5.4",
    "zsa": "0.6.0",
    "zsa-react": "0.2.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@next/eslint-plugin-next": "16.1.6",
    "@playwright/test": "1.58.2",
    "@tailwindcss/postcss": "4.1.18",
    "@tailwindcss/typography": "0.5.19",
    "@types/node": "25.2.3",
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "@types/react-syntax-highlighter": "15.5.13",
    "@typescript-eslint/eslint-plugin": "8.55.0",
    "@typescript-eslint/parser": "8.55.0",
    "dotenv": "17.2.4",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "postcss-import": "16.1.1",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3"
  },
  "overrides": {
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@heroui/shared-utils",
      "es5-ext",
      "sharp",
      "unrs-resolver"
    ]
  }
}
