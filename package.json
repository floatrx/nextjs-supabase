{
  "name": "next-supabase",
  "description": "Next.js Blog with Supabase",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "gen:types": "pwd && source .env && supabase gen types typescript --project-id=$SUPABASE_PROJECT_ID > types/supabase.ts",
    "eslint": "next lint --fix",
    "type:check": "tsc --noEmit",
    "type:build": "tsc -b",
    "lint": "lint-staged",
    "pretty": "prettier --write  '**/*.{ts,tsx}'",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts?(x)": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.css": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@heroui/accordion": "2.2.14",
    "@heroui/avatar": "2.2.13",
    "@heroui/breadcrumbs": "2.2.13",
    "@heroui/button": "2.2.17",
    "@heroui/card": "2.2.16",
    "@heroui/chip": "2.2.13",
    "@heroui/dropdown": "2.3.17",
    "@heroui/image": "2.2.10",
    "@heroui/input": "2.4.17",
    "@heroui/link": "2.2.14",
    "@heroui/navbar": "2.2.15",
    "@heroui/pagination": "2.2.15",
    "@heroui/popover": "2.3.17",
    "@heroui/select": "2.4.17",
    "@heroui/snippet": "2.2.18",
    "@heroui/spinner": "2.2.14",
    "@heroui/switch": "2.2.15",
    "@heroui/system": "2.4.13",
    "@heroui/tabs": "2.2.14",
    "@heroui/theme": "2.4.13",
    "@hookform/resolvers": "^3.6.0",
    "@mdxeditor/editor": "^3.31.0",
    "@react-aria/ssr": "^3.9.8",
    "@react-aria/visually-hidden": "^3.8.22",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "autoprefixer": "10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.2.11",
    "intl-messageformat": "^10.7.16",
    "lucide-react": "^0.503.0",
    "next": "15.3.1",
    "next-themes": "^0.4.6",
    "nextjs-toploader": "^3.8.16",
    "nuqs": "^2.4.3",
    "postcss": "8.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.56.1",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "server-only": "^0.0.1",
    "sonner": "^2.0.3",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.2.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.3",
    "zod-validation-error": "^3.4.0",
    "zsa": "^0.6.0",
    "zsa-react": "^0.2.3"
  },
  "devDependencies": {
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "15.3.1",
    "@types/node": "22.15.2",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "@tailwindcss/typography": "^0.5.16",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2"
  }
}
